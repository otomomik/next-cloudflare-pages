    "@cloudflare/next-on-pages": "^0.2.0",
    "@cloudflare/workers-types": "^4.20221111.1",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "vercel": "^28.13.1",
